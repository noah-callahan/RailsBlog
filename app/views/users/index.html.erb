<div class="container">
    <h1>Users</h1>


    <% @users.each do |user| %>
        <div class="post" >
            <h2><%= link_to(user.fname + " " + user.lname, user_path(user) ) %></h2>
            <h4>Username: <%=user.uname%></h4>
            <h4>Email: <%=user.email%></h4>
        </div>
    <% end %>
</div>